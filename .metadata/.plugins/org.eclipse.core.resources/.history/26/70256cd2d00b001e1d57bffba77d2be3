package application;

import java.util.Scanner;

import entites.Triangle;

public class Main {

	public static void main(String[] args) {
		Scanner scan = new Scanner(System.in);
		Triangle x, y;
		x = new Triangle();
		y = new Triangle();
		
		x.a = scan.nextDouble();
		x.b = scan.nextDouble();
		x.c = scan.nextDouble();
		
		y.a = scan.nextDouble();
		y.b = scan.nextDouble();
		y.c = scan.nextDouble();
		
		double areaX = x.area();
		double areay = x.area();
		scan.close();
		}

}
